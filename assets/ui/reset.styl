//== css重新设置
html,
body,
div,
h1,
h2,
h3,
h4,
h5,
h6,
hr,
p,
blockquote,
dl,
dt,
dd,
ul,
li,
pre,
fieldset,
legend,
button,
input,
textarea,
form,
th,
td {
  margin: 0;
  padding: 0;
  $prefix(box-sizing, border-box);
}

html {
  font-size 16px
  font-family: 'Helvetica Neue', Helvetica, sans-serif;
}

ul, ol {
  list-style none
}

h1, h2, h3, h4, h5, h6 {
  font-size 1rem
  font-weight normal
}

table {
  border-collapse: collapse;
  border-spacing: 0px;
}